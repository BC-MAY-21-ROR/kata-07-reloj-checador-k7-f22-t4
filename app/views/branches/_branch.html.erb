<div id="<%= dom_id branch %>">
  <tbody>
    <tr>
      <td><%= branch.name %></td>
      <td><%= branch.address %></td>
      <td>
        <%= link_to '<i class="fa-solid fa-pen-to-square"></i>'.html_safe, edit_branch_path(branch.id) %>
        <%= link_to '<i class="fa-solid fa-trash-can"></i>'.html_safe, branch_path(branch.id), data: { turbo_method: :delete } %>
      </td>
    </tr>
  </tbody>

</div>
