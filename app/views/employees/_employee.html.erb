<div id="<%= dom_id employee %>">
  <p>
    <strong>Name:</strong>
    <%= employee.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= employee.email %>
  </p>

  <p>
    <strong>Position:</strong>
    <%= employee.position %>
  </p>

  <p>
    <strong>Private code:</strong>
    <%= employee.private_code %>
  </p>

  <p>
    <strong>Active:</strong>
    <%= employee.active %>
  </p>

  <p>
    <strong>Branch:</strong>
    <%= employee.branch.name %>
  </p>

</div>
