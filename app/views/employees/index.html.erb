<div class="sub-container">
  <div class="search">
    <input type="text" placeholder="#Search">
  </div>
  <table class="table">
    <thead>
      <th>Name</th>
      <th>Email</th>
      <th>Position</th>
      <th>#</th>
      <th>Private <br> number</th>
      <th>Branch</th>
      <th>Active</th>
      <th> </th>
    </thead> 
    <% @employees.each do |employee| %>
      <%= render employee %>
    <% end %>
  </table>
  <div class="button-cont">
    <%= button_to '+', new_employee_path, method: :get, class: 'button' %>
  </div>
</div>
